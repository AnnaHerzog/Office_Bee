<div class="container">

  <h1> Profile <%= @user.first_name%> <%= @user.last_name %></h1>


  <% if @user.photo.attached? %>
  <%= cl_image_tag @user.photo.key, height: 300, width: 400, crop: :fill %>
  <% end %>
   <!-- <p> First Name <%#= @user.first_name%> Last Name <%#= @user.last_name%></p> -->

  <br>
  <p> Bio <%= @user.bio%></p>

  <%= link_to "Update your profile information",  edit_user_registration_path %>
  <%= link_to "delete Account", user_registration_path, method: :delete, data: { confirm: "You sure?" } %>

</div>

